   "source": [
    "@lru_cache()\n",
    "def fibonacci(n):\n",
    "    if n==0:\n",
    "        x = 0\n",
    "    elif n==1:\n",
    "        x = 1\n",
    "    else:\n",
    "        x = fibonacci(n-1) + fibonacci(n-2)\n",
    "    return x"
   ]
  },
